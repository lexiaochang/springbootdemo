<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chris.springboot.mapper.GradeMapper" >

    <select id="getByGradeNm" parameterType="String" resultType="com.chris.springboot.entity.GradeEntity">
        select * from grade where grade_nm=#{name}
    </select>

</mapper>